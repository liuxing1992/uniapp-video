
.videoList {
	position: fixed;
	width: 100%;
	height: 100%;
	top: 0;
	left: 0;
	background-color: #007AFF;
}
.swiper-box {
	width: 100%;
	height: 100%;
}
.swiper {
	width: 100%;
	height: 100%;
}
.swiper-item{
	width: 100%;
	height: 100%;
	z-index: 19;
}
.left-box{
	z-index: 20;
	position: absolute;
	bottom: 50px;
	left: 20px;
}
.right-box{
	z-index: 20;
	position: absolute;
	bottom: 50px;
	right: 20px;
}

